<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
        integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <title>search</title>
    <style>
        .class3 {
            background-color: lightsalmon
        }

        #div01 {
            border-style: solid
        }

        #div02 {
            border-style: solid
        }

        #div03 {
            border-style: solid
        }

        #div04 {
            border-style: solid
        }
        #div1{
    padding: 20px 20px 20px;
}
    </style>



</head>

<body onload="pageLoaded()">
    <div class="container ">
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <a class="navbar-brand" href="http://localhost:1337/rent/home">Home</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
                aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                <div class="navbar-nav">
                    <a class="nav-item nav-link active" href="http://localhost:1337/rent/paginate">Search<span
                            class="sr-only">(current)</span></a>
                    <a class="nav-item nav-link " href="http://localhost:1337/rent/create">Create</a>
                    <a class="nav-item nav-link " href="http://localhost:1337/rent">Admin</a>

                </div>
            </div>
        </nav>

        <div class="jumbotron jumbotron-fluid">
            <div class="container">
                <h1 class="display-4">Real Estate Rental System</h1>
                <p class="lead">Real Estate Rental Management System is a web-based system to facilitate flatshare and
                    finding roommates.
                </p>
            </div>
        </div>


        

        <div class="row">

            <div id="div01" class="card col-12 col-sm-8 col-lg-8 col-xl-8">

                <div class="row">
                        <% rents.forEach( function(model) { %>
                    <div class="col-6 col-sm-6 col-lg-6 col-xl-6" id="div1">

                        

                        <img class="img-fluid img-thumbnail" src="<%= model.url %>" value="cover" width="250"
                            height="250" />

                            <br><h5><a href="http://localhost:1337/rent/details/<%= model.id %>""><%= model.title %></a></h5>
                <br>
                        <ul class="list-group">
                                <li class="list-group-item"><label><h5>Estate:</h5></label> <%= model.estate %></li>
                                <li class="list-group-item"><label><h5>Gross Area:</h5></label> <%= model.area %> sq feet</li>
                                <li class="list-group-item"><label><h5>Rent:</h5></label> $<%= model.rent %></li>

                              </ul>
                                
                    
                    
                    </div>
                    <% }); %>
                    
                    <div id=" div01" class="card col-6 col-sm-6 col-lg-6 col-xl-6">


                        </div>


                </div>





            </div>
         
   
       
                    <div id="div01" class="card col-12 col-sm-4 col-lg-4 col-xl-4">
                        <!---->
                  
<!--form-->
                            <form action="/rent/paginate" method="get">
                    <div>
                        <h1>Property Search</h1>
                        <div class="row">
                            <div class="col-12 col-sm-4 col-lg-4 col-xl-3">
                                <label>Estate:</label></div>
                            <div class="col-12 col-sm-4 col-lg-4 col-xl-9">
                                <select type="string" class="form-control" name="estate" id="estate">

                                    <option value="Robinson Heights">Robinson Heights</option>
                                    <option value="Hoi deen Court">Hoi deen Court</option>
                                    <option value="Lime Stardom">Lime Stardom</option>
                                    <option value="AKVO Hotel">AKVO Hotel</option>
                                </select></div>
                        </div>
                        <br><!--bedroom-->
                        <div class="row">
                            <div class="col-12 col-sm-4 col-lg-4 col-xl-3">
                                <label>Bedroom:</label></div>
                            <div class="col-12 col-sm-4 col-lg-4 col-xl-9">
                                <input type="number" min=1 max=4 id="bedroom" class="form-control" name="bedroom">
                            </div>
                        </div>
                        <br>
                        <div class="row">
                            <div class="col-12 col-sm-4 col-lg-4 col-xl-6">
                                <label>Area (Sq feet):</label></div>
                        </div>

                        <div class="row">
                            <div class="col-12 col-sm-4 col-lg-4 col-xl-2">
                                <label>Min:</label></div>
                            <div class="col-12 col-sm-4 col-lg-4 col-xl-4">
                                <input type="number" id="minarea" min=30 max=2000 class="form-control"
                                    name="minarea" ></div>


                            <div class="col-12 col-sm-4 col-lg-4 col-xl-2">
                                <label>Max:</label></div>
                            <div class="col-12 col-sm-4 col-lg-4 col-xl-4">
                                <input type="number" id="maxarea" min=30 max=2000 class="form-control"
                                    name="maxarea" ></div>
                        </div>
                        <br>
                        <div class="row">
                            <div class="col-12 col-sm-4 col-lg-4 col-xl-6">
                                <label>Rent (HK$):</label></div>
                        </div>

                        <div class="row">
                            <div class="col-12 col-sm-4 col-lg-4 col-xl-2">
                                <label>Min:</label></div>
                            <div class="col-12 col-sm-4 col-lg-4 col-xl-4">
                                <input type="number" id="minrent" min=5000 max=60000 class="form-control"
                                    name="minrent"  ></div>


                            <div class="col-12 col-sm-4 col-lg-4 col-xl-2">
                                <label>Max:</label></div>
                            <div class="col-12 col-sm-4 col-lg-4 col-xl-4">
                                <input type="number" id="maxrent" min=5000 max=60000 class="form-control"
                                    name="maxrent"  ></div>
                        </div>
                        <br>
                   
                           
                        <button type="submit" class="btn btn-primary">Search</button>    
                                     

                    

                    </div>
                </form>
          
                </div>
                 
                </div>

          



                <% const current = parseInt(req.query.page) || 1; %>

                <nav aria-label="Page navigation example">
                    <ul class="pagination">
                        <li class="page-item">
                            <a class="page-link" href="/rent/paginate/?page=<%= Math.max(current-1, 1) %>">Previous</a>
                        </li>

                        <% for (i = 1; i <= count; i++) { %>
                        <li class="page-item">
                            <a class="page-link" href="/rent/paginate/?page=<%= i %>"><%= i %></a>
                        </li>
                        <% } %>

                        <li class="page-item">
                            <a class="page-link" href="/rent/paginate/?page=<%= Math.min(current+1, count) %>">Next</a>
                        </li>
                    </ul>
                </nav>


            </div>





            <script>




            </script>
            <script src=" https://code.jquery.com/jquery-3.3.1.slim.min.js"
                integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo"
                crossorigin="anonymous"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"
                integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1"
                crossorigin="anonymous"></script>
            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
                integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
                crossorigin="anonymous"></script>
</body>

</html>